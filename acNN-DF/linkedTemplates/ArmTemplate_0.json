			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Pad10')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Flowlet",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [
						{
							"name": "DerivedColumn1"
						},
						{
							"name": "input1"
						},
						{
							"name": "output1"
						}
					],
					"scriptLines": [
						"input(output(",
						"          col1 as string",
						"     ),",
						"     order: 0) ~> input1",
						"input1 derive(col1 = lpad(col1, 10, '0')) ~> DerivedColumn1",
						"DerivedColumn1 output() ~> output1"
					]
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureKeyVault')]",
