		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "acNN-DF"
		},
		"AcJava_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AcJava'"
		},
		"AcNN_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AcNN'"
		},
		"AzureBlobStorage_Target_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Target'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureTables_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTables'"
		},
		"StorageTable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageTable'"
		},
		"acNN_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'acNN_DB'"
		},
		"AcJava_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://acnn-java.azurewebsites.net"
		},
		"AcNN_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://acnn.azurewebsites.net"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://acnndatalake.dfs.core.windows.net"
		},
		"TestRestServier_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://acnn.azurewebsites.net/api/OutputTrigger?code=q4xMDtMkSEuJWQe/9rmDancm7VQ/B6NX8UNAitEkaPH1QhRmfaiOow=="
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ac-kv.vault.azure.net/"
		}
